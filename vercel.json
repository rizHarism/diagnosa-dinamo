{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "css/*",
            "use": "@vercel/static"
        },
        {
            "src": "js/*",
            "use": "@vercel/static"
        },
        {
            "src": "data/*",
            "use": "@vercel/static"
        },
        {
            "src": "page/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}